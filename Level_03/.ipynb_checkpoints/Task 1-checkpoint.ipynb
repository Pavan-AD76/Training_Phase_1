    "try:\n",
    "    a=int(input(\"Enter a number:\"))\n",
    "    b=int(input(\"Enter a number:\"))\n",
    "    num=a/b\n",
    "except ZeroDivisionError:\n",
    "    print(\"Enter a number except zero\")\n",
    "else:\n",
    "    print(\"Ans:\",num)\n",
    "finally:\n",
    "    print(\"Execution Done\")"
   ]
