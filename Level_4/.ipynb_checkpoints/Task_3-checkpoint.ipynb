   ],
   "source": [
    "import concurrent.futures\n",
    "def print_numbers():\n",
    "    print([1, 2, 3])\n",
    "def print_letters():\n",
    "    print([\"A\", \"B\", \"C\"])\n",
    "with concurrent.futures.ThreadPoolExecutor() as executor:\n",
    "    executor.submit(print_numbers)\n",
    "    executor.submit(print_letters)"
   ]
